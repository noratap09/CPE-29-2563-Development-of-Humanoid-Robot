<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Interaction_control"
    android:background="@android:color/background_dark">

    <TextView
        android:id="@+id/txt_stt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="32dp"
        android:text="TextView"
        android:textColor="@android:color/white"
        android:textSize="36sp"
        app:layout_constraintBottom_toTopOf="@+id/img_mount"
        app:layout_constraintEnd_toStartOf="@+id/img_eye_R"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toEndOf="@+id/img_eye_L"
        android:visibility="invisible"/>

    <ImageView
        android:id="@+id/img_eye_L"
        android:layout_width="140dp"
        android:layout_height="183dp"
        android:layout_marginStart="50dp"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/img_eye_R"
        android:layout_width="140dp"
        android:layout_height="183dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="50dp"
        android:layout_marginRight="50dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/img_mount"
        android:layout_width="219dp"
        android:layout_height="84dp"
        android:layout_marginBottom="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/img_eye_R"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toEndOf="@+id/img_eye_L"/>

    <ImageView
        android:id="@+id/img_eyebrow_L"
        android:layout_width="200dp"
        android:layout_height="70dp"
        app:layout_constraintStart_toStartOf="@+id/img_eye_L"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/img_shy_L"
        android:layout_width="140dp"
        android:layout_height="70dp"
        app:layout_constraintStart_toStartOf="@+id/img_eye_L"
        app:layout_constraintTop_toBottomOf="@+id/img_eye_L" />

    <ImageView
        android:id="@+id/img_shy_R"
        android:layout_width="140dp"
        android:layout_height="70dp"
        app:layout_constraintEnd_toEndOf="@+id/img_eye_R"
        app:layout_constraintTop_toBottomOf="@+id/img_eye_R" />

    <ImageView
        android:id="@+id/img_eyebrow_R"
        android:layout_width="200dp"
        android:layout_height="70dp"
        app:layout_constraintEnd_toEndOf="@+id/img_eye_R"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/img_ui_detail"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:alpha=".75"
        android:visibility="invisible"/>
</androidx.constraintlayout.widget.ConstraintLayout>